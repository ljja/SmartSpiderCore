<?xml version="1.0" encoding="utf-8"?>
<SpiderEngine xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <Title>58家装服务-企业用户</Title>
  <Description>东北-辽宁省-庄河市-pld.58.com</Description>

  <TableXPath>//*[@id="infolist"]/table</TableXPath>
  <IsSplitTable>true</IsSplitTable>

  <InputList>
    <Input xsi:type="HttpInput">
      <Encoding>utf-8</Encoding>
      <ContentType>text/html; charset=utf-8</ContentType>
      <Host>pld.58.com</Host>
      <Method>GET</Method>
      <Sleep>0</Sleep>
      <CookieClean>30</CookieClean>
      <Referer>http://pld.58.com/jiazhuang/</Referer>
      <Accept>text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8</Accept>
      <UserAgent>Mozilla/5.0 (Windows NT 6.3; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/35.0.1916.153 Safari/537.36</UserAgent>
      <Headers>
        <NameValue>
          <Name>Accept-Language</Name>
          <Value>zh-CN,zh;q=0.8,en;q=0.6</Value>
        </NameValue>
        <NameValue>
          <Name>Cache-Control</Name>
          <Value>max-age=0</Value>
        </NameValue>
      </Headers>
      <UrlEnumerator>
        <UrlFormat>http://pld.58.com/jiazhuang/pn{MEMBER}</UrlFormat>
        <EnumeratorRuleList>
          <EnumeratorRule xsi:type="ForEnumeratorRule">
            <Feature>{MEMBER}</Feature>
            <Start>1</Start>
            <End>1</End>
            <Step>1</Step>
            <Direction>true</Direction>
          </EnumeratorRule>
        </EnumeratorRuleList>
      </UrlEnumerator>
    </Input>
  </InputList>

  <FieldList>

	<Field>
      <Title>区域</Title>
      <DataName>区域</DataName>
      <Require>true</Require>
      <Sort>0</Sort>
      <Rule>
        <Rule xsi:type="FixedValueRule" >
          <Text>东北</Text>
        </Rule>
      </Rule>
    </Field>
  
    <Field>
      <Title>省份</Title>
      <DataName>省份</DataName>
      <Require>true</Require>
      <Sort>5</Sort>
      <Rule>
        <Rule xsi:type="FixedValueRule" >
          <Text>辽宁省</Text>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>城市</Title>
      <DataName>城市</DataName>
      <Require>true</Require>
      <Sort>10</Sort>
      <Rule>
        <Rule xsi:type="FixedValueRule" >
          <Text>庄河市</Text>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>信息类别</Title>
      <DataName>信息类别</DataName>
      <Require>true</Require>
      <Sort>20</Sort>
      <Rule>
        <Rule xsi:type="FixedValueRule" >
          <Text>公司</Text>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>公司名称</Title>
      <DataName>公司名称</DataName>
      <Require>false</Require>
      <Index>1</Index>
      <Sort>30</Sort>
      <Rule>
        <Rule xsi:type="SubStringRule">
          <Begin>onclick="clickLog('from=List_</Begin>
          <End>/a&gt;</End>
        </Rule>
        <Rule xsi:type="SubStringRule">
          <Begin>&gt;</Begin>
          <End>&lt;</End>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>网邻通</Title>
      <DataName>网邻通</DataName>
      <Require>false</Require>
      <Index>1</Index>
      <Sort>40</Sort>
      <Rule>
        <Rule xsi:type="SubStringRule">
          <Begin>网邻通指数:</Begin>
          <End>]</End>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>用户评分</Title>
      <DataName>用户评分</DataName>
      <Require>false</Require>
      <Index>2</Index>
      <Sort>50</Sort>
      <Rule>
        <Rule xsi:type="SubStringRule">
          <Begin>star00" title="</Begin>
          <End>"</End>
        </Rule>
        <Rule xsi:type="ReplaceRule">
          <Origin>分</Origin>
          <Target></Target>
        </Rule>
      </Rule>
    </Field>

    <Field>
      <Title>联系方式</Title>
      <DataName>联系方式</DataName>
      <Require>false</Require>
      <Index>3</Index>
      <Sort>60</Sort>
      <Rule>
        <Rule xsi:type="InnerTextRule" />
        <Rule xsi:type="TrimRule" />
        <Rule xsi:type="ReplaceRule">
          <Origin>免费预约</Origin>
          <Target></Target>
        </Rule>
        <Rule xsi:type="ReplaceRule">
          <Origin>保障足</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="ReplaceRule">
          <Origin>可评价</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="ReplaceRule">
          <Origin>扫一扫</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="ReplaceRule">
          <Origin>×</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="ReplaceRule">
          <Origin>进移动网站二维码来自</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="ReplaceRule">
          <Origin>58微站通</Origin>
          <Target></Target>
        </Rule>

        <Rule xsi:type="TrimRule" />
        
      </Rule>
    </Field>

  </FieldList>

  <OutputList>
    <Output xsi:type="CsvOutput">
      <Path>data/东北.辽宁省.庄河市.pld.58.com.家装服务.企业.csv</Path>
    </Output>
  </OutputList>

</SpiderEngine>